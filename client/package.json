{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "echo 'MongoDB does not require generation'",
    "db:push": "echo 'MongoDB schemas are handled in code'",
    "db:seed:generate": "tsx scripts/seed-local.ts",
    "db:seed": "tsx scripts/seed-import.ts",
    "db:seed:direct": "tsx scripts/seed.ts",
    "db:migrate": "echo 'MongoDB does not use migrations'",
    "db:studio": "echo 'Use MongoDB Compass or Studio 3T for MongoDB GUI'",
    "db:setup": "node scripts/setup-mongodb.js",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "analyze": "ANALYZE=true next build",
    "analyze:view": "ANALYZE=true next build && open .next/static/chunks/analyze.html",
    "deploy:check": "npm run lint && npm run type-check && npm run build",
    "postinstall": "echo 'MongoDB setup complete'"
  },
  "dependencies": {
    "@next-auth/mongodb-adapter": "^1.1.3",
