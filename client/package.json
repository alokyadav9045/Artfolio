    "@upstash/ratelimit": "^1.0.1",
    "@vercel/kv": "^1.0.1",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.540.0",
